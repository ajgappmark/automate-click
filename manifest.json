{
  "author": "igorlogius",
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "description": "automate click actions when opening a website",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "automate-click",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "version": "1.2.3"
}
